  "typesVersions": {
    "*": {
      "*": [
        "./dist/signals.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "unpkg": "dist/signals.esm.js",
  "jsdelivr": "dist/signals.esm.js",
  "scripts": {
    "build": "tsup && cross-env NODE_ENV=production tsup --clean=false --treeshake --sourcemap=false",

{
  "name": "essor",
  "version": "0.0.15-beta.12",
  "description": "",
  "type": "module",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/estjs/essor",
  "bugs": "https://github.com/estjs/essor/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/essor.git"
  },
  "author": "jiangxd <jiangxd2016@gmail.com>",
  "funding": "https://github.com/sponsors/estjs",
  "files": [
    "dist",
    "jsx-runtime.d.ts",
    "types"
  ],
  "main": "./dist/essor-client.cjs.js",
  "module": "./dist/essor-client.esm.js",
  "types": "./dist/essor-client.d.ts",
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/essor-client.d.ts",
        "require": "./dist/essor-client.cjs.js",
        "import": "./dist/essor-client.esm.js"
      },
      "node": {
        "types": "./dist/essor-server.d.ts",
        "require": "./dist/essor-server.cjs.js",
        "import": "./dist/essor-server.esm.js"
      },
      "server": {
        "types": "./dist/essor-server.d.ts",
        "require": "./dist/essor-server.cjs.js",
        "import": "./dist/essor-server.esm.js"
      },
      "default": {
        "types": "./dist/essor-client.d.ts",
        "require": "./dist/essor-client.cjs.js",
        "import": "./dist/essor-client.esm.js"
      }
    },
    "./jsx-runtime": {
      "types": "./jsx-runtime.d.ts",
      "default": "./dist/essor.esm.js"
    },
    "./jsx-dev-runtime": {
      "types": "./types/jsx.d.ts",
      "default": "./dist/essor.esm.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/essor.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "unpkg": "dist/essor.esm.js",
  "jsdelivr": "dist/essor.esm.js",
  "scripts": {
    "build": "tsup && cross-env NODE_ENV=production tsup --clean=false --treeshake --sourcemap=false",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@estjs/server": "workspace:*",
    "@estjs/shared": "workspace:*",
    "@estjs/signals": "workspace:*",
    "@estjs/template": "workspace:*"
  }
}

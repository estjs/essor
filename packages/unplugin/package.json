{
  "name": "@estjs/unplugin",
  "version": "0.0.14",
  "description": "",
  "type": "module",
  "keywords": ["unplugin", "vite", "essor", "estjs", "webpack", "rollup", "transform"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/essor.git"
  },
  "files": ["dist"],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "require": "./dist/astro.cjs",
      "import": "./dist/astro.js"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "require": "./dist/rspack.cjs",
      "import": "./dist/rspack.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": ["./dist/*", "./*"]
    }
  },
  "scripts": {
    "build": "tsup",
    "build:fix": "node ./scripts/postbuild.js",
    "dev": "cross-env NODE_ENV=production tsup --watch"
  },
  "peerDependencies": {
    "esbuild": "*",
    "rollup": "^4",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.27.1",
    "@estjs/babel-plugin": "workspace:*",
    "unplugin": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "chalk": "^5.4.1",
    "fast-glob": "^3.3.3",
    "rollup": "^4.40.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "webpack": "^5.99.8"
  }
}
